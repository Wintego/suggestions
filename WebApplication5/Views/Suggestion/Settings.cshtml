@{     
    var symbolCount = WebApplication5.Controllers.SuggestionController.SymbolCount;
    var suggestionCount = WebApplication5.Controllers.SuggestionController.SuggestionCount;
    
}
@model List<int>
<form method="post">  @*asp-action="Index"*@
    <label>Количество символов</label>
    <input type="number" min="1" value="@symbolCount" name="symbolCount"/>
    <label>Количество подсказок</label>
    <input type="number" min="1" value="@suggestionCount" name="suggestionCount"/>
    <input type="submit" value="OK" />
</form>